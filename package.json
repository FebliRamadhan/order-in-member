{
  "name": "express-es6-rest-api",
  "version": "0.3.0",
  "description": "Online test from member.id",
  "main": "dist",
  "scripts": {
    "dev": " NODE_ENV=development nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "db:create": "NODE_ENV=development sequelize db:create",
    "db:migrate": "NODE_ENV=development sequelize db:migrate",
    "db:seed": "NODE_ENV=development sequelize db:seed:all",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": "developit/express-es6-rest-api",
  "author": "Jason Miller <jason@developit.ca>",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "config": "^3.3.0",
    "cors": "^2.7.1",
    "express": "^4.13.3",
    "fs": "0.0.1-security",
    "http": "0.0.0",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "md5": "^2.2.1",
    "morgan": "^1.8.0",
    "mysql2": "^2.1.0",
    "path": "^0.12.7",
    "resource-router-middleware": "^0.6.0",
    "sequelize": "^5.21.5",
    "sequelize-cli": "^5.5.1",
    "sequelize-hierarchy": "^2.0.4",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.26.0",
    "eslint": "^4.18.2",
    "nodemon": "^1.19.4"
  }
}
